<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.ahandlingunititemdelivery.AHandlingUnitItemDeliveryActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.api_outbound_delivery_srv_entities.AHandlingUnitItemDeliveryType" />
        <variable name="aHandlingUnitItemDeliveryType" type="AHandlingUnitItemDeliveryType"/>
        <variable name="handler" type="com.golive.godrive.btppublic.mdui.ahandlingunititemdelivery.AHandlingUnitItemDeliveryDetailFragment" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:id="@+id/item_detail_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                app:layout_behavior="@string/appbar_scrolling_view_behavior">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitInternalID"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitInternalID).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitItem"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitItem).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="Batch"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.batch).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="DeliveryDocument"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.deliveryDocument).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="DeliveryDocumentItem"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.deliveryDocumentItem).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitExternalID"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitExternalID).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitNestedInternalID"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitNestedInternalID).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitNrOfAuxPackgMat"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitNrOfAuxPackgMat).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitNumberOfSerialnumb"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitNumberOfSerialnumb).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitQuantity"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitQuantity).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitQuantityUnit"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitQuantityUnit).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="HandlingUnitTypeOfContent"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.handlingUnitTypeOfContent).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="Material"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.material).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="MaterialByCustomer"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.materialByCustomer).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="MaterialName"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.materialName).toString()}" />

                    <com.sap.cloud.mobile.fiori.misc.KeyValueCell
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:keyText="SerialNumberProfile"
                        app:valueText="@{aHandlingUnitItemDeliveryType.getOptionalValue(AHandlingUnitItemDeliveryType.serialNumberProfile).toString()}" />

                </LinearLayout>
            </androidx.core.widget.NestedScrollView>
        </androidx.coordinatorlayout.widget.CoordinatorLayout>
    </LinearLayout>
</layout>